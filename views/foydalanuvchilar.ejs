<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <title>Hamidullo Mirzaikromov</title>
</head>

<script src="/js/bootstrap.js"></script>
<script src="/js/jquery.js"></script>
<script src="/js/popover.js"></script>

<body>
    <div class="container">
        <div class="row">
            <div class="col">
                <table class="table-sm table-striped  table-hover table-bordered mt-5 w-100">
                    <thead class="thead-dark">
                        <tr  class="bg-secondary text-white">
                            <th style="text-align: center;">Foydalanuvchi nomi</th>
                            <th style="text-align: center;">Tog'ri javoblar soni</th>
                            <th style="text-align: center;">Faollik vaqti</th>
                        </tr>
                    </thead>
                    <tbody  style="background-color: white;">
                       <%let index = 1;%>
                        <%users.forEach(function(user) {%>
                            <tr>
                                <td><%= index;%>. <%= user.nomi%></td>
                                <%index++;%>
                                <td style="text-align: center;"><%= user.tjs%></td>
                                <td style="text-align: center;"><%= user.vaqt%></td>
                            </tr>
                        <%})%>
                        
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
</html>